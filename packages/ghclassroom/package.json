{
  "name": "ghclassroom",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "version": "1.0.0-beta.1",
  "main": "dist/index.js",
  "bin": "dist/cli.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "copybin": "cpy ./src/cli.cjs ./dist/ --flat",
    "copyconf": "cpy ./src/cli-config/ghcl.config.mjs ./dist/cli-config/ --flat",
    "prebuild": "yarn lint && yarn copyconf && yarn copybin",
    "build": "yarn prebuild && tsc --build --force"
  },
  "exports": {
    "./messaging": "./dist/messaging/index.js"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^17.0.30",
    "@types/stream-json": "^1.7.2",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.14.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "csv": "^6.0.5",
    "inquirer": "^8.2.4",
    "messaging": "file:../messaging",
    "stream-json": "^1.7.4",
    "tools": "file:../tools",
    "yargs": "^17.4.1"
  },
  "typesVersions": {
    "*": {
      "messaging": [
        "./dist/messaging/index.d.ts"
      ]
    }
  }
}
