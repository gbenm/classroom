{
  "name": "csvw-cli",
  "packageManager": "yarn@3.2.0",
  "version": "1.0.0-beta.1",
  "author": "Josu√© Galeano",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbenm/classroom.git"
  },
  "homepage": "https://github.com/gbenm/classroom",
  "keywords": [
    "csv",
    "cli",
    "write"
  ],
  "bin": {
    "csvwriter": "./dist/cli.cjs"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "cpybin": "cpy ./src/cli.cjs ./dist --flat",
    "copyconf": "cpy ./src/cli-config/csvw.config.mjs ./dist/cli-config --flat",
    "prebuild": "yarn lint && yarn cpybin && yarn copyconf",
    "build": "yarn prebuild && tsc --build --force"
  },
  "exports": {
    ".": "./dist/index.js",
    "./messaging": "./dist/messaging.js"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.30",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.14.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "typesVersions": {
    "*": {
      "messaging": [
        "./dist/messaging.d.ts"
      ]
    }
  },
  "dependencies": {
    "csv": "^6.0.5",
    "messaging": "1.0.0-beta",
    "tools": "1.0.0-beta",
    "yargs": "^17.4.1"
  },
  "files": [
    "dist"
  ]
}
